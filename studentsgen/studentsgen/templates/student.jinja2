{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
  <h1><span class="font-semi-bold"> {{student.name}} </span></h1>
  <h2><span class="font-semi-bold"> {{group.name}} </span></h2>

  {% for course, subject in data %}
  	<li>
  	  <a href="{{request.route_url('student_course', s=student.id, c=course.id)}}"> {{subject.name}} </a>
  	</li>
  {% endfor %}
</div>
{% endblock content %}
